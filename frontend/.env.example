# Backend API Configuration
REACT_APP_BACKEND_URL=https://your-backend-domain.com
REACT_APP_API_BASE_URL=https://your-backend-domain.com/api

# Web3 Configuration
REACT_APP_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id

# Network Configuration
REACT_APP_DEFAULT_CHAIN_ID=1
REACT_APP_SUPPORTED_CHAINS=1,42161,10,137,8453

# Smart Contract Addresses (will be updated after deployment)
REACT_APP_GAS_OPTIMIZATION_HOOK_ADDRESS=0x...
REACT_APP_POOL_MANAGER_ADDRESS=0x...
REACT_APP_COST_CALCULATOR_ADDRESS=0x...
REACT_APP_CROSS_CHAIN_MANAGER_ADDRESS=0x...
REACT_APP_GAS_PRICE_ORACLE_ADDRESS=0x...

# External API Keys
REACT_APP_COINGECKO_API_KEY=your_coingecko_api_key
REACT_APP_DEFILLAMA_API_KEY=your_defillama_api_key

# Analytics & Monitoring
REACT_APP_ANALYTICS_ENABLED=true
REACT_APP_MIXPANEL_TOKEN=your_mixpanel_token
REACT_APP_SENTRY_DSN=your_sentry_dsn

# Feature Flags
REACT_APP_ENABLE_CROSS_CHAIN_OPTIMIZATION=true
REACT_APP_ENABLE_USD_DISPLAY=true
REACT_APP_ENABLE_ANALYTICS_DASHBOARD=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Development
REACT_APP_DEBUG=true
REACT_APP_MOCK_DATA=false